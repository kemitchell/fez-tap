{
  "name": "fez-tap",
  "version": "0.1.1",
  "description": "TAP tests for your fez. Keep your tests ultra portable by using Tape. Run them on the backend with Fez, and on the frontend with anything else.",
  "main": "index.js",
  "scripts": {
    "test": "DEBUG=* node test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/miketheprogrammer/fez-tap.git"
  },
  "keywords": [
    "fez",
    "tape",
    "tap",
    "test",
    "browser",
    "testing"
  ],
  "testling": {
    "files": "test.js",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  },
  "author": "Michael Hernandez -- michael.hernandez1988@gmail.com",
  "license": "MIT License",
  "bugs": {
    "url": "https://github.com/miketheprogrammer/fez-tap/issues"
  },
  "homepage": "https://github.com/miketheprogrammer/fez-tap",
  "devDependencies": {
    "debug": "~0.7.4"
  },
  "dependencies": {
    "tape": "~2.4.2",
    "tap-parser": "~0.4.1",
    "bluebird": "~1.0.5",
    "debug": "~0.7.4"
  }
}
